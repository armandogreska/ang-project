{
  "/wordpress.local": {
    "target": "https://wordpress.local",
    "secure": false,
    "changeOrigin": true,
    "logLevel": "debug",
    "pathRewrite": {
      "^/wordpress.local": "https://wordpress.local"
    },
    "headers": {
      "Host": "wordpress.local",
      "Accept": "application/json"
    }
  }
}
