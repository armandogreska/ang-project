{
  "name": "ang-project",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "ng build",
    "format": "prettier --write \"src/**/*.*\" && npx sort-package-json",
    "lint": "eslint \"src/**/*.*\"",
    "lint:fix": "eslint \"src/**/*.*\" --fix",
    "ng": "ng",
    "npm-check-updates": "npx npm-check-updates -u && npm install",
    "outdated": "npm outdated",
    "prepare": "husky",
    "serve:ssr:ang-project": "node dist/ang-project/server/server.mjs",
    "start": "ng serve",
    "test": "ng test",
    "watch": "ng build --watch --configuration development"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.*.{ts,tsx,html,css,scss,json}": "prettier --write",
    "**/*.{ts,tsx,html,css,scss,json}": [
      "prettier --write",
      "eslint"
    ]
  },
  "dependencies": {
    "@angular/common": "19.2.6",
    "@angular/compiler": "19.2.6",
    "@angular/core": "19.2.6",
    "@angular/forms": "19.2.6",
    "@angular/platform-browser": "19.2.6",
    "@angular/platform-browser-dynamic": "19.2.6",
    "@angular/platform-server": "19.2.6",
    "@angular/router": "19.2.6",
    "@angular/ssr": "19.2.8",
    "bootstrap": "^5.3.5",
    "express": "4.21.2",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.2.8",
    "@angular/cli": "19.2.8",
    "@angular/compiler-cli": "19.2.6",
    "@eslint/js": "9.24.0",
    "@types/express": "5.0.1",
    "@types/jasmine": "5.1.7",
    "@types/node": "22.14.1",
    "angular-eslint": "19.3.0",
    "eslint": "9.24.0",
    "eslint-config-prettier": "10.1.2",
    "eslint-plugin-prettier": "5.2.6",
    "eslint-plugin-unused-imports": "4.1.4",
    "globals": "16.0.0",
    "husky": "9.1.7",
    "jasmine-core": "5.6.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "lint-staged": "15.5.1",
    "prettier": "3.5.3",
    "typescript": "5.8.3",
    "typescript-eslint": "8.30.1"
  },
  "packageManager": "pnpm@10.9.0+sha512.0486e394640d3c1fb3c9d43d49cf92879ff74f8516959c235308f5a8f62e2e19528a65cdc2a3058f587cde71eba3d5b56327c8c33a97e4c4051ca48a10ca2d5f"
}
