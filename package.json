{
  "name": "ang-project",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "ng build",
    "format": "prettier --write \"src/**/*.*\" && npx sort-package-json",
    "lint": "eslint \"src/**/*.*\"",
    "lint:fix": "eslint \"src/**/*.*\" --fix",
    "ng": "ng",
    "npm-check-updates": "npx npm-check-updates -u && npm install",
    "outdated": "npm outdated",
    "prepare": "husky",
    "serve:ssr:ang-project": "node dist/ang-project/server/server.mjs",
    "start": "ng serve",
    "test": "ng test",
    "watch": "ng build --watch --configuration development"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.*": "prettier --write",
    "**/*.{ts,tsx,html,css,scss,json}": [
      "prettier --write",
      "eslint"
    ]
  },
  "dependencies": {
    "@angular/animations": "18.2.7",
    "@angular/cdk": "18.2.7",
    "@angular/common": "18.2.7",
    "@angular/compiler": "18.2.7",
    "@angular/core": "18.2.7",
    "@angular/forms": "18.2.7",
    "@angular/material": "18.2.7",
    "@angular/platform-browser": "18.2.7",
    "@angular/platform-browser-dynamic": "18.2.7",
    "@angular/platform-server": "18.2.7",
    "@angular/router": "18.2.7",
    "@angular/ssr": "18.2.7",
    "express": "4.21.0",
    "rxjs": "7.8.1",
    "tslib": "2.7.0",
    "zone.js": "0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "18.2.7",
    "@angular/cli": "18.2.7",
    "@angular/compiler-cli": "18.2.7",
    "@eslint/js": "9.12.0",
    "@types/express": "4.17.21",
    "@types/jasmine": "5.1.4",
    "@types/node": "22.5.5",
    "angular-eslint": "18.3.1",
    "eslint": "9.12.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-unused-imports": "4.1.4",
    "globals": "15.10.0",
    "husky": "9.1.6",
    "jasmine-core": "5.3.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "4.0.2",
    "karma-jasmine-html-reporter": "1.7.0",
    "lint-staged": "15.2.10",
    "prettier": "3.3.3",
    "typescript": "5.5.4",
    "typescript-eslint": "8.6.0"
  }
}
